{% for lo_interface in lo_interfaces|dict2items %}
delete interfaces {{ lo_interface.key }} unit {{ lo_interface.value.unit }} family inet address {{ lo_interface.value.address }}
{% endfor %}
{% for ebgp_group in ebgp_groups %}
delete security authentication-key-chains key-chain {{ ebgp_group.key_chain_name }} key 1 secret {{ ebgp_group.key_chain_secret }}
delete security authentication-key-chains key-chain {{ ebgp_group.key_chain_name }} key 1 start-time now
delete security authentication-key-chains key-chain {{ ebgp_group.key_chain_name }} key 1 algorithm md5
delete protocols bgp group {{ ebgp_group.name }} type external
delete protocols bgp group {{ ebgp_group.name }} multipath multiple-as
delete protocols bgp group {{ ebgp_group.name }} local-as {{ ebgp_group.local_as_number }}
delete protocols bgp group {{ ebgp_group.name }} local-address {{ lo_interfaces[ebgp_group.local_lo].address }}
delete protocols bgp group {{ ebgp_group.name }} family evpn signaling
delete protocols bgp group {{ ebgp_group.name }} bfd-liveness-detection minimum-interval {{ ebgp_group.bfd_minimum_interval }}
delete protocols bgp group {{ ebgp_group.name }} bfd-liveness-detection multiplier {{ ebgp_group.multiplier }}
delete protocols bgp group {{ ebgp_group.name }} bfd-liveness-detection authentication key-chain {{ ebgp_group.key_chain_name }}
delete protocols bgp group {{ ebgp_group.name }} bfd-liveness-detection authentication algorithm keyed-md5
{% for neighbor in ebgp_group.neighbors %}
delete protocols bgp group {{ ebgp_group.name }} neighbor {{ neighbor.peer }} peer-as {{ neighbor.asn }} multihop
{% endfor %}
{% endfor %}